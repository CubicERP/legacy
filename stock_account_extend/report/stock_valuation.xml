<?xml version="1.0"?>
<openerp>
    <data>

        <record id="action_stock_valuation_report" model="ir.actions.act_window">
            <field name="name">Stock Quant Valuation</field>
            <field name="res_model">base.report.params</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

        <record model="ir.actions.report.xml" id="stock_valuation_report">
            <field name="name">Stock Valuation Excel</field>
            <field name="report_name">report.stock.valuation.excel</field>
            <field name="model">product.template</field>
            <field name="report_type">cubicReport.xlsx</field>
            <field name="report_file">stock_account_extend/report/stock_valuation.xlsx</field>
            <field name="menu_show">1</field>
            <field name="act_window_id" ref="action_stock_valuation_report"/>
            <field name="xlcustom_params_src">[
{'name': 'fecha', 'type': 'date', 'args': ['Fecha'], 'kwargs':{}, 'default':time.strftime('%Y-%m-%d'), 'string':'Fecha', 'required': '1'},
{'name': 'tipo', 'type': 'selection', 'args': [[("'internal'",'Internos'),
                                                ("'customer'",'Clientes'),
                                                ("'transit'",'Tránsito'),
                                                ("'supplier'",'Proveedores'),
                                                ("'inventory'",'Inventarios'),
                                                ("'production'",'Producción'),
                                                ("'procurement'",'Abastecimientos')]], 'kwargs':{}, 'string':'Tipo', 'required': '1'},
]</field>
        </record>

        <record id="action_stock_location_valuation_report" model="ir.actions.act_window">
            <field name="name">Stock Quant Valuation by Location</field>
            <field name="res_model">base.report.params</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

        <record model="ir.actions.report.xml" id="stock_location_valuation_report">
            <field name="name">Stock Location Valuation Excel</field>
            <field name="report_name">report.stock.location.valuation.excel</field>
            <field name="model">product.template</field>
            <field name="report_type">cubicReport.xlsx</field>
            <field name="report_file">stock_account_extend/report/stock_valuation_location.xlsx</field>
            <field name="menu_show">1</field>
            <field name="act_window_id" ref="action_stock_location_valuation_report"/>
            <field name="xlcustom_params_src">[
{'name': 'fecha', 'type': 'date', 'args': ['Fecha'], 'kwargs':{}, 'default':time.strftime('%Y-%m-%d'), 'string':'Fecha', 'required': '1'},
{'name': 'tipo', 'type': 'selection', 'args': [[("'internal'",'Internos'),
                                                ("'customer'",'Clientes'),
                                                ("'transit'",'Tránsito'),
                                                ("'supplier'",'Proveedores'),
                                                ("'inventory'",'Inventarios'),
                                                ("'production'",'Producción'),
                                                ("'procurement'",'Abastecimientos'),
                                                ("'%'",'Todas las Ubicaciones'),]], 'kwargs':{}, 'string':'Tipo', 'required': '1'},
]</field>
        </record>

        <record id="action_stock_detail_valuation_report" model="ir.actions.act_window">
            <field name="name">Stock Move Valuation Detailed</field>
            <field name="res_model">base.report.params</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

        <record model="ir.actions.report.xml" id="stock_detail_valuation_report">
            <field name="name">Stock Detail Valuation Excel</field>
            <field name="report_name">report.stock.detail.valuation.excel</field>
            <field name="model">product.template</field>
            <field name="report_type">cubicReport.xlsx</field>
            <field name="report_file">stock_account_extend/report/stock_valuation_detail.xlsx</field>
            <field name="menu_show">1</field>
            <field name="act_window_id" ref="action_stock_detail_valuation_report"/>
            <field name="xlcustom_params_src">[
{'name': 'fecha_ini', 'type': 'date', 'args': ['Fecha Inicio'], 'kwargs':{}, 'default':time.strftime('%Y-01-01'), 'string':'Fecha Inicio', 'required': '1'},
{'name': 'fecha_fin', 'type': 'date', 'args': ['Fecha Fin'], 'kwargs':{}, 'default':time.strftime('%Y-%m-%d'), 'string':'Fecha Fin', 'required': '1'},
{'name': 'tipo_orig', 'type': 'selection', 'args': [[("'internal'",'Internos'),
                                                ("'customer'",'Clientes'),
                                                ("'transit'",'Tránsito'),
                                                ("'supplier'",'Proveedores'),
                                                ("'inventory'",'Inventarios'),
                                                ("'production'",'Producción'),
                                                ("'procurement'",'Abastecimientos'),
                                                ("'%'",'Todas las Ubicaciones'),]], 'kwargs':{}, 'string':'Tipo Origen', 'required': '1'},
{'name': 'tipo_dest', 'type': 'selection', 'args': [[("'internal'",'Internos'),
                                                ("'customer'",'Clientes'),
                                                ("'transit'",'Tránsito'),
                                                ("'supplier'",'Proveedores'),
                                                ("'inventory'",'Inventarios'),
                                                ("'production'",'Producción'),
                                                ("'procurement'",'Abastecimientos'),]], 'kwargs':{}, 'string':'Tipo Destino', 'required': '1'},
]</field>
        </record>

    </data>
</openerp>
