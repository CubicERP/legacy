<?xml version="1.0" encoding="UTF-8"?>
<openerp>
   <data>
   	
        <record model='ir.ui.view' id='view_move_form'>
			<field name='name'>stock.move.form.inherit.account.extend</field>
			<field name='model'>stock.move</field>
			<field name='inherit_id' ref='stock.view_move_form'/>
			<field name='arch' type='xml'>
                <field name="partner_id" position="after">
                    <field name="period_id" groups="account.group_account_user"/>
                    <field name="account_move_id" groups="account.group_account_user"/>
                </field>
            </field>
        </record>
        
        <record model='ir.ui.view' id='view_move_picking_form'>
			<field name='name'>stock.move.form.inherit.account.extend</field>
			<field name='model'>stock.move</field>
			<field name='inherit_id' ref='stock.view_move_picking_form'/>
			<field name='arch' type='xml'>
                <field name="date_expected" position="after">
                    <field name="period_id" groups="account.group_account_user"/>
                    <field name="account_move_id" groups="account.group_account_user"/>
                </field>
            </field>
        </record>
        
        <record model='ir.ui.view' id='view_picking_type_form'>
			<field name='name'>Picking Types.inherit.account.extend</field>
			<field name='model'>stock.picking.type</field>
			<field name='inherit_id' ref='stock.view_picking_type_form'/>
			<field name='arch' type='xml'>
                <xpath expr="//field[@name='warehouse_id']" position="after">
                    <field name="journal_id" />
                </xpath>
            </field>
        </record>
	</data>
</openerp>
